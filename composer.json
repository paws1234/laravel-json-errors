{
  "name": "Paws1234/laravel-json-errors",
  "description": "Streamlined JSON error handling for Laravel APIs.",
  "keywords": ["laravel", "error-handling", "json", "api"],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Paws1234\\LaravelJsonErrors\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paws1234\\LaravelJsonErrors\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1 || ^8.2 || ^8.3",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Paws1234\\LaravelJsonErrors\\JsonErrorsServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
